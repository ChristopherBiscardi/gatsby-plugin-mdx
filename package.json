    "react-chartjs-2": "^2.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "cross-env": "^5.0.5"
  },
  "peerDependencies": {
    "@mdx-js/loader": "^0.9.0",
    "@mdx-js/mdx": "^0.9.0",
    "gatsby": "^1.0.0"
